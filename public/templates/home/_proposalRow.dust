<div class="proposal_list_item" {#isLast}data-is-last="true"{/isLast}>
  <div class="proposal_image">
    <img src="/{image.name}">
  </div>

  <div class="proposal_info">
    <div class="title">
      {title}
    </div>

    <div class="proposal_summary">{summary}</div>

    <div class="learn_more">
      <a class="btn btn-default btn-sm" href="/p/{_id}/more?wrap=true" target="_blank">LEARN MORE</a>
    </div>

  </div>
</div>
